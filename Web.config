<configuration>
  <appSettings>
    <!--este localhost lo obtenemos del puerto que nos otorga IIS
		y nos servirá para obtener recursos propios del servidor-->
    <add key="FileServer" value="http://localhost:50554/" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.8" />
    <httpRuntime targetFramework="4.8" />
  </system.web>
</configuration>